{
	"extends": "fastify-tsconfig",
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["dom", "dom.iterable", "ESNext"],
		"outDir": "app/dist",
		"sourceMap": true,
		"strict": true,
		"resolveJsonModule": true,
		"typeRoots": ["node_modules/@types", "app/src/types"]
	},
	"include": ["app/src/**/*.ts"],
	"exclude": ["app/frontend-scripts/**/*.ts"]
}
